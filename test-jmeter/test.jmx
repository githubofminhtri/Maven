          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion" enabled="true">
            <stringProp name="BeanShellAssertion.query">String totalNumber = vars.get(&quot;iDs_matchNr&quot;);
int count = Integer.parseInt(totalNumber);
log.info(&quot;&quot;+count);
// File path
String filePath = &quot;data.csv&quot;; 

// Data to write
try {

  // Open file for write 
  PrintWriter writer = new PrintWriter(filePath, &quot;UTF-8&quot;);  
  
  // Write data line by line
  for (int i =1; i&lt;=count;i++){
	String id = vars.get(&quot;iDs_&quot;+i);
	String email = vars.get (&quot;emails_&quot;+i);
	log.info(&quot;&quot;+id);
	log.info(&quot;&quot;+email);
	 writer.println(id+&quot;,&quot;+email);
	}

  // Close the file
  writer.close();

  // Log success
  log.info(&quot;Data written to CSV successfully!&quot;);

} catch (FileNotFoundException e) {
  log.error(&quot;Error writing CSV&quot;, e);
} catch (UnsupportedEncodingException e) {
  log.error(&quot;Encoding error&quot;, e);
}</stringProp>
